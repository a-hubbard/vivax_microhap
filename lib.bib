@article{gryseelsHighMobilityLow2015,
  title = {High {{Mobility}} and {{Low Use}} of {{Malaria Preventive Measures Among}} the {{Jarai Male Youth Along}} the {{Cambodia-Vietnam Border}}},
  author = {Gryseels, Charlotte and Peeters Grietens, Koen and Dierickx, Susan and Xuan, Xa Nguyen and Uk, Sambunny and Bannister-Tyrrell, Melanie and Trienekens, Suzan and Ribera, Joan Muela and Hausmann-Muela, Susanna and Gerrets, René and D'Alessandro, Umberto and Sochantha, Tho and Coosemans, Marc and Erhart, Annette},

  author = {Ghansah, Anita and Tiedje, Kathryn E. and Argyropoulos, Dionne C. and Onwona, Christiana O. and Deed, Samantha L. and Labbé, Frédéric and Oduro, Abraham R. and Koram, Kwadwo A. and Pascual, Mercedes and Day, Karen P.},
  date = {2023},

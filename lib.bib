}

@article{ochwedoHyperprevalenceSubmicroscopicPlasmodium2021,
  title = {Hyper-Prevalence of Submicroscopic {{Plasmodium}} Falciparum Infections in a Rural Area of Western {{Kenya}} with Declining Malaria Cases},
  author = {Ochwedo, Kevin O. and Omondi, Collince J. and Magomere, Edwin O. and Olumeh, Julius O. and Debrah, Isaiah and Onyango, Shirley A. and Orondo, Pauline W. and Ondeto, Benyl M. and Atieli, Harrysone E. and Ogolla, Sidney O. and Githure, John and Otieno, Antony C. A. and Githeko, Andrew K. and Kazura, James W. and Mukabana, Wolfgang R. and Guiyan, Yan},
  date = {2021-12-20},

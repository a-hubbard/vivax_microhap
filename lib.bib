  author = {Sumner, Kelsey M. and Freedman, Elizabeth and Abel, Lucy and Obala, Andrew and Pence, Brian W. and Wesolowski, Amy and Meshnick, Steven R. and Prudhomme-O'Meara, Wendy and Taylor, Steve M.},

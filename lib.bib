  author = {Miller, Robin H. and Hathaway, Nicholas J. and Kharabora, Oksana and Mwandagalirwa, Kashamuka and Tshefu, Antoinette and Meshnick, Steven R. and Taylor, Steve M. and Juliano, Jonathan J. and Stewart, V. Ann and Bailey, Jeffrey A.},
  date = {2017-12-16},

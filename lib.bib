@article{gottdenkerAnthropogenicLandUse2014,
  title = {Anthropogenic {{Land Use Change}} and {{Infectious Diseases}}: {{A Review}} of the {{Evidence}}},
  shorttitle = {Anthropogenic {{Land Use Change}} and {{Infectious Diseases}}},
  author = {Gottdenker, Nicole L. and Streicker, Daniel G. and Faust, Christina L. and Carroll, C. R.},
  date = {2014-12-01},
  journaltitle = {EcoHealth},
  shortjournal = {EcoHealth},

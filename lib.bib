@article{parkerMalariaEcologyThailandMyanmar2015,
  title = {Malaria Ecology along the {{Thailand-Myanmar}} Border},
  author = {Parker, Daniel M. and Carrara, Verena I. and Pukrittayakamee, Sasithon and McGready, Rose and Nosten, François H.},

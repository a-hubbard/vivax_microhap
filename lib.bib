@article{verityEstimatingNumberSubpopulations2016,
  title = {Estimating the {{Number}} of {{Subpopulations}} ({{K}}) in {{Structured Populations}}},
  author = {Verity, Robert and Nichols, Richard A.},
  date = {2016-08},
  journaltitle = {Genetics},
  shortjournal = {Genetics},
  volume = {203},
  number = {4},

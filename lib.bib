  author = {Beleza, Sandra and Campos, Joana and Lopes, Jailson and Araújo, Isabel Inês and Almada, Ana Hoppfer and family=Silva, given=António Correia, prefix=e, useprefix=false and Parra, Esteban J. and Rocha, Jorge},

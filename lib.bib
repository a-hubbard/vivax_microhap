@thesis{keppleIdentificationAlternativeInvasion2020,
  title = {Identification of {{Alternative Invasion Pathways}} of {{Plasmodium}} Vivax in {{East Africa}}},
  author = {Kepple, Daniel D.},
  date = {2020},
  institution = {The University of North Carolina at Charlotte},
  location = {Charlotte, NC},
  abstract = {Plasmodium vivax malaria is a neglected tropical disease, despite being more geographically widespread than any other form of malaria. P. vivax was previously thought to be rare or absent in Africa because people of African descent often lack the Duffy blood group antigen, known as the Duffy antigen-chemokine receptor (DARC). DARC is a glycoprotein on the surface of red blood cells (RBCs) that allows P. vivax to bind and invade human erythrocytes. The documentation of P. vivax infections in different parts of Africa where Duffy-negative individuals are predominant suggested that there are alternative pathways for P. vivax to invade human erythrocytes. Duffy-negative individuals may be just as fit as Duffy-positive individuals and are no longer resistant to P. vivax malaria. The genetic characteristics and erythrocyte invasion mechanisms of P. vivax in Duffy-negative infections are largely unknown due to the less frequent occurrence in Africa and the complicated biology of P. vivax. My thesis research aims to investigate the genetic attributes of P. vivax in Duffy-negative individuals as compared to Duffy-positive individuals and identify alternative ligands for erythrocyte invasion through comparative transcriptomics and in vitro binding assays of major surface protein genes to clarify invasion functions. This research is critical for the development of new malaria intervention protocols and will assist in development of a stable P. vivax machine.},
  file = {/Users/alfred/Zotero/storage/Z4UPQUW2/Kepple Dissertation Proposal.docx}
}

@article{kepplePlasmodiumVivaxDuffyNegative2021,
  title = {Plasmodium Vivax {{From Duffy-Negative}} and {{Duffy-Positive Individuals Share Similar Gene Pools}} in {{East Africa}}},
  author = {Kepple, Daniel and Hubbard, Alfred and Ali, Musab M. and Abargero, Beka R. and Lopez, Karen and Pestana, Kareen and Janies, Daniel A. and Yan, Guiyun and Hamid, Muzamil Mahdi and Yewhalaw, Delenasaw and Lo, Eugenia},
  date = {2021-10-28},
  journaltitle = {The Journal of Infectious Diseases},
  shortjournal = {J Infect Dis},
  volume = {224},
  number = {8},
  eprint = {33534886},
  eprinttype = {pmid},
  pages = {1422--1431},
  issn = {1537-6613},
  doi = {10.1093/infdis/jiab063},
  abstract = {Plasmodium vivax malaria was thought to be rare in Africa, but an increasing number of P. vivax cases reported across Africa and in Duffy-negative individuals challenges this dogma. The genetic characteristics of P. vivax in Duffy-negative infections, the transmission of P. vivax in East Africa, and the impact of environments on transmission remain largely unknown. This study examined genetic and transmission features of P. vivax from 107 Duffy-negative and 305 Duffy-positive individuals in Ethiopia and Sudan. No clear genetic differentiation was found in P. vivax between the 2 Duffy groups, indicating between-host transmission. P. vivax from Ethiopia and Sudan showed similar genetic clusters, except samples from Khartoum, possibly due to distance and road density that inhibited parasite gene flow. This study is the first to show that P. vivax can transmit to and from Duffy-negative individuals and provides critical insights into the spread of P. vivax in sub-Saharan Africa.},
  langid = {english},
  pmcid = {PMC8557672},
  keywords = {Africa Eastern,Duffy Blood-Group System,Duffy-negative,East Africa,Erythrocytes,Gene Pool,genetic diversity,Genetic Variation,Humans,Malaria Vivax,notion,Plasmodium vivax,Receptors Cell Surface,Sudan,transmission pathways},
  file = {/Users/alfred/Zotero/storage/2QVNN3T6/Kepple et al. - 2021 - Plasmodium vivax From Duffy-Negative and Duffy-Pos.pdf;/Users/alfred/Zotero/storage/KP5DLS9H/jiab063_suppl_supplementary_table_1.xlsx;/Users/alfred/Zotero/storage/MWEC5KFK/jiab063_suppl_supplementary_table_2.xlsx;/Users/alfred/Zotero/storage/MYXNIECQ/jiab063_suppl_supplementary_materials.docx;/Users/alfred/Zotero/storage/PWNQN3EG/jiab063_suppl_supplementary_table_4.xlsx;/Users/alfred/Zotero/storage/QN63FYJT/jiab063_suppl_supplementary_figure_s1_s3.pdf;/Users/alfred/Zotero/storage/R8U6BSFB/jiab063_suppl_supplementary_table_3.xlsx}

  keywords = {Arabidopsis,Genetic Variation,Genome Human,Genome Plant,Humans,Metagenomics,notion,Polymorphism Single Nucleotide,population genomics,single-nucleotide polymorphisms,software,Software,Web Browser},
  file = {/Users/alfred/Zotero/storage/7K4B9UPV/Pfeifer et al. - 2014 - PopGenome an efficient Swiss army knife for popul.pdf;/Users/alfred/Zotero/storage/BLYHL7EI/Whole_genome_analyses_using_VCF_files.pdf;/Users/alfred/Zotero/storage/LA6M87TQ/An_introduction_to_the_PopGenome_package.pdf}

@article{sakamotoAnalysisRapidExpansion2009,
  title = {Analysis of Rapid Expansion of Inland Aquaculture and Triple Rice-Cropping Areas in a Coastal Area of the {{Vietnamese Mekong Delta}} Using {{MODIS}} Time-Series Imagery},
  author = {Sakamoto, Toshihiro and Van Phung, Cao and Kotera, Aikihiko and Nguyen, Khang Duy and Yokozawa, Masayuki},

  keywords = {Adolescent,Anemia,Child,Child Preschool,Communicable Disease Control,Databases Factual,Diagnostic Tests Routine,Female,Humans,Kenya,Malaria,Male,notion,Parasitology,Plasmodium,Population Surveillance,Schools,Surveys and Questionnaires,Telecommunications},

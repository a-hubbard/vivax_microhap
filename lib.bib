  shorttitle = {Exposure to {{Diverse Plasmodium}} Falciparum {{Genotypes Shapes}} the {{Risk}} of {{Symptomatic Malaria}} in {{Incident}} and {{Persistent Infections}}},
  author = {Sumner, Kelsey M. and Freedman, Elizabeth and Mangeni, Judith N. and Obala, Andrew A. and Abel, Lucy and Edwards, Jessie K. and Emch, Michael and Meshnick, Steven R. and Pence, Brian W. and Prudhomme-O'Meara, Wendy and Taylor, Steve M.},
  date = {2021-10-05},

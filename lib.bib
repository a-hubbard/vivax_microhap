  author = {Santamaría, Ana María and Vásquez, Vanessa and Rigg, Chystrie and Samudio, Franklyn and Moreno, Dianik and Romero, Luis and Saldaña, Azael and Chaves, Luis Fernando and Calzada, José Eduardo},

@article{decesareFlexibleCosteffectiveGenomic2024,
  title = {Flexible and Cost-Effective Genomic Surveillance of {{P}}. Falciparum Malaria with Targeted Nanopore Sequencing},
  author = {family=Cesare, given=Mariateresa, prefix=de, useprefix=true and Mwenda, Mulenga and Jeffreys, Anna E. and Chirwa, Jacob and Drakeley, Chris and Schneider, Kammerle and Mambwe, Brenda and Glanz, Karolina and Ntalla, Christina and Carrasquilla, Manuela and Portugal, Silvia and Verity, Robert J. and Bailey, Jeffrey A. and Ghinai, Isaac and Busby, George B. and Hamainza, Busiku and Hawela, Moonga and Bridges, Daniel J. and Hendry, Jason A.},

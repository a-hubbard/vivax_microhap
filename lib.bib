@article{changLowParasiteConnectivity2021,
  title = {Low Parasite Connectivity among Three Malaria Hotspots in {{Thailand}}},
  author = {Chang, Hsiao-Han and Chang, Meng-Chun and Kiang, Mathew and Mahmud, Ayesha S. and Ekapirat, Nattwut and Engø-Monsen, Kenth and Sudathip, Prayuth and Buckee, Caroline O. and Maude, Richard J.},

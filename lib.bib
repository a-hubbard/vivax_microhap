@article{dudaniecDealingUncertaintyLandscape2016,
  title = {Dealing with Uncertainty in Landscape Genetic Resistance Models: A Case of Three Co-Occurring Marsupials},
  shorttitle = {Dealing with Uncertainty in Landscape Genetic Resistance Models},
  author = {Dudaniec, Rachael Y. and Wilmer, Jessica Worthington and Hanson, Jeffrey O. and Warren, Matthew and Bell, Sarah and Rhodes, Jonathan R.},
  date = {2016},
  journaltitle = {Molecular Ecology},

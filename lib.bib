@article{larremoreNetworkApproachAnalyzing2013,
  title = {A Network Approach to Analyzing Highly Recombinant Malaria Parasite Genes},
  author = {Larremore, Daniel B. and Clauset, Aaron and Buckee, Caroline O.},
  date = {2013},
  journaltitle = {PLoS computational biology},

@article{leinenkugelCharacterisationLandSurface2013,
  title = {Characterisation of Land Surface Phenology and Land Cover Based on Moderate Resolution Satellite Data in Cloud Prone Areas - {{A}} Novel Product for the {{Mekong Basin}}},
  author = {Leinenkugel, Patrick and Kuenzer, Claudia and Oppelt, Natascha and Dech, Stefan},

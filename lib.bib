  author = {Wiscovitch-Russo, R. and Narganes-Stordes, Y. and Cano, R. J. and Toranzos, G. A.},
  date = {2019-09},
  journaltitle = {International Microbiology: The Official Journal of the Spanish Society for Microbiology},

  author = {Roy, Avijit and Hartung, John S. and Schneider, William L. and Shao, Jonathan and Leon, Guillermo and Melzer, Michael J. and Beard, Jennifer J. and Otero-Colina, Gabriel and Bauchan, Gary R. and Ochoa, Ronald and Brlansky, Ronald H.},
  date = {2015},

  author = {Tatem, Andrew J. and Jia, Peng and Ordanovich, Dariya and Falkner, Michael and Huang, Zhuojie and Howes, Rosalind and Hay, Simon I. and Gething, Peter W. and Smith, David L.},
  date = {2017-01},
  journaltitle = {The Lancet. Infectious Diseases},

@article{stoddardRoleHumanMovement2009,
  title = {The {{Role}} of {{Human Movement}} in the {{Transmission}} of {{Vector-Borne Pathogens}}},
  author = {Stoddard, Steven T. and Morrison, Amy C. and Vazquez-Prokopec, Gonzalo M. and Paz Soldan, Valerie and Kochel, Tadeusz J. and Kitron, Uriel and Elder, John P. and Scott, Thomas W.},
  date = {2009-07-21},
  journaltitle = {PLoS Neglected Tropical Diseases},

@article{matobaP53RegulatesMitochondrial2006,
  title = {P53 {{Regulates Mitochondrial Respiration}}},
  author = {Matoba, Satoaki and Kang, Ju-Gyeong and Patino, Willmar D. and Wragg, Andrew and Boehm, Manfred and Gavrilova, Oksana and Hurley, Paula J. and Bunz, Fred and Hwang, Paul M.},
  date = {2006-06-16},

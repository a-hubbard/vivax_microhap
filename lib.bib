@article{dasilvaTargetedWholegenomeSequencing2023,
  title = {Targeted and Whole-Genome Sequencing Reveal a North-South Divide in {{P}}. Falciparum Drug Resistance Markers and Genetic Structure in {{Mozambique}}},

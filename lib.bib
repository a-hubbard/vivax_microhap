  keywords = {Adult,Biomarkers,Brazil,Child,Cohort Studies,Early Diagnosis,Humans,Immunoglobulin G,Infection Control,Longitudinal Studies,Malaria Vivax,Melanesia,notion,Plasmodium vivax,Prevalence,Sensitivity and Specificity,Serologic Tests,Thailand,Time Factors},

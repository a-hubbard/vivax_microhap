  author = {Wesolowski, Amy and Taylor, Aimee R. and Chang, Hsiao-Han and Verity, Robert and Tessema, Sofonias and Bailey, Jeffrey A. and Alex Perkins, T. and Neafsey, Daniel E. and Greenhouse, Bryan and Buckee, Caroline O.},

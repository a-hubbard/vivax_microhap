@article{salgadoPrevalenceDensityAsymptomatic2021,
  title = {The Prevalence and Density of Asymptomatic {{Plasmodium}} Falciparum Infections among Children and Adults in Three Communities of Western {{Kenya}}},
  author = {Salgado, Christina and Ayodo, George and Macklin, Michael D. and Gould, Meetha P. and Nallandhighal, Srinivas and Odhiambo, Eliud O. and Obala, Andrew and O'Meara, Wendy Prudhomme and John, Chandy C. and Tran, Tuan M.},
  date = {2021-09-17},

  pmcid = {PMC7936321},
  keywords = {gene flow,invasive species,landscape genetics,notion,random forest,vector control},
  file = {/Users/alfred/Zotero/storage/HGIJBUAY/Pless et al. - 2021 - A machine-learning approach to map landscape conne.pdf}
}

@article{policelliLakeChadTotal2018,
  title = {Lake {{Chad Total Surface Water Area}} as {{Derived}} from {{Land Surface Temperature}} and {{Radar Remote Sensing Data}}},
  author = {Policelli, Frederick and Hubbard, Alfred and Jung, Hahn Chul and Zaitchik, Ben and Ichoku, Charles},
  date = {2018-02-07},
  journaltitle = {Remote Sensing},
  volume = {10},
  number = {252},
  issn = {2072-4292},

@article{kalaghatgiFamilyJoiningFastDistanceBased2016,
  title = {Family-{{Joining}}: {{A Fast Distance-Based Method}} for {{Constructing Generally Labeled Trees}}},
  shorttitle = {Family-{{Joining}}},
  author = {Kalaghatgi, Prabhav and Pfeifer, Nico and Lengauer, Thomas},
  date = {2016-10},
  journaltitle = {Molecular Biology and Evolution},
  shortjournal = {Mol Biol Evol},
  volume = {33},
  number = {10},

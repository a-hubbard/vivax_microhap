@article{haleSamplingMicrosatelliteBasedPopulation2012,
  title = {Sampling for {{Microsatellite-Based Population Genetic Studies}}: 25 to 30 {{Individuals}} per {{Population Is Enough}} to {{Accurately Estimate Allele Frequencies}}},
  shorttitle = {Sampling for {{Microsatellite-Based Population Genetic Studies}}},
  author = {Hale, Marie L. and Burg, Theresa M. and Steeves, Tammy E.},
  date = {2012-09-12},
  journaltitle = {PLoS ONE},
  shortjournal = {PLoS One},
  volume = {7},
  number = {9},

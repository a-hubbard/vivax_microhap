  author = {Reiner, Robert C. and Le Menach, Arnaud and Kunene, Simon and Ntshalintshali, Nyasatu and Hsiang, Michelle S. and Perkins, T. Alex and Greenhouse, Bryan and Tatem, Andrew J. and Cohen, Justin M. and Smith, David L.},

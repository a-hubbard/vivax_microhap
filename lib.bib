@article{hamreLackConsistentMalaria2020,
  title = {Lack of {{Consistent Malaria Incidence Hotspots}} in a {{Highland Kenyan Area During}} a 10-{{Year Period}} of {{Very Low}} and {{Unstable Transmission}}},
  author = {Hamre, Karen E. S. and Hodges, James S. and Ayodo, George and John, Chandy C.},

@article{kamilovaCollimonasFungivoransUnpredicted2007,
  title = {Collimonas Fungivorans, an Unpredicted in Vitro but Efficient in Vivo Biocontrol Agent for the Suppression of Tomato Foot and Root Rot},
  author = {Kamilova, Faina and Leveau, Johan H.J. and Lugtenberg, Ben},

@article{dellicourUsingPhylogeographicApproaches2019,
  title = {Using Phylogeographic Approaches to Analyse the Dispersal History, Velocity and Direction of Viral Lineages — {{Application}} to Rabies Virus Spread in {{Iran}}},
  author = {Dellicour, Simon and Troupin, Cécile and Jahanbakhsh, Fatemeh and Salama, Akram and Massoudi, Siamak and Moghaddam, Madjid K. and Baele, Guy and Lemey, Philippe and Gholami, Alireza and Bourhy, Hervé},
  date = {2019},

@article{osbornePlasmodiumFalciparumPopulation2024,
  title = {Plasmodium Falciparum Population Dynamics in {{East Africa}} and Genomic Surveillance along the {{Kenya-Uganda}} Border},
  author = {Osborne, Ashley and Mańko, Emilia and Waweru, Harrison and Kaneko, Akira and Kita, Kiyoshi and Campino, Susana and Gitaka, Jesse and Clark, Taane G.},

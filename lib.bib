@article{hamiltonEvolutionExpansionMultidrugresistant2019,
  title = {Evolution and Expansion of Multidrug-Resistant Malaria in Southeast {{Asia}}: A Genomic Epidemiology Study},
  shorttitle = {Evolution and Expansion of Multidrug-Resistant Malaria in Southeast {{Asia}}},
  author = {Hamilton, William L. and Amato, Roberto and family=Pluijm, given=Rob W., prefix=van der, useprefix=true and Jacob, Christopher G. and Quang, Huynh Hong and Thuy-Nhien, Nguyen Thanh and Hien, Tran Tinh and Hongvanthong, Bouasy and Chindavongsa, Keobouphaphone and Mayxay, Mayfong and Huy, Rekol and Leang, Rithea and Huch, Cheah and Dysoley, Lek and Amaratunga, Chanaki and Suon, Seila and Fairhurst, Rick M. and Tripura, Rupam and Peto, Thomas J. and Sovann, Yok and Jittamala, Podjanee and Hanboonkunupakarn, Borimas and Pukrittayakamee, Sasithon and Chau, Nguyen Hoang and Imwong, Mallika and Dhorda, Mehul and Vongpromek, Ranitha and Chan, Xin Hui S. and Maude, Richard J. and Pearson, Richard D. and Nguyen, T. and Rockett, Kirk and Drury, Eleanor and Gonçalves, Sónia and White, Nicholas J. and Day, Nicholas P. and Kwiatkowski, Dominic P. and Dondorp, Arjen M. and Miotto, Olivo},
  date = {2019-09},
  journaltitle = {The Lancet. Infectious Diseases},

@article{laliberteValidationFunctionalConnectivity2020,
  title = {Validation of Functional Connectivity Modeling: {{The Achilles}}’ Heel of Landscape Connectivity Mapping},
  shorttitle = {Validation of Functional Connectivity Modeling},
  author = {Laliberté, Jérôme and St-Laurent, Martin-Hugues},

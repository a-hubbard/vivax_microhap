  keywords = {Genotyping Techniques,Geography,Humans,Malaria Falciparum,notion,Plasmodium falciparum},
  file = {/Users/alfred/Zotero/storage/CJ7WLKPR/Molina-Cruz et al. - 2021 - A genotyping assay to determine geographic origin .pdf}
}

@article{mollentzeBayesianApproachInferring2014,
  title = {A {{Bayesian}} Approach for Inferring the Dynamics of Partially Observed Endemic Infectious Diseases from Space-Time-Genetic Data},

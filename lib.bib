}

@article{tessemaSpatialGeneticClustering2020,
  title = {Spatial and Genetic Clustering of {{Plasmodium}} Falciparum and {{Plasmodium}} Vivax Infections in a Low-Transmission Area of {{Ethiopia}}},
  author = {Tessema, Sofonias K. and Belachew, Mulualem and Koepfli, Cristian and Lanke, Kjerstin and Huwe, Tiffany and Chali, Wakweya and Shumie, Girma and Mekuria, Elias F. and Drakeley, Chris and Gadisa, Endalamaw and Greenhouse, Bryan and Bousema, Teun and Tadesse, Fitsum G.},

@article{smithHighSpatialResolution2009,
  title = {High Spatial Resolution Data Acquisition for the Geosciences: Kite Aerial Photography},
  author = {Smith, J.S. and Chandler, J. and Rose, J.},
  date = {2009},
  journaltitle = {Earth Surface Processes and Landforms},
  volume = {34},
  eprint = {264460600012},
  eprinttype = {pmid},

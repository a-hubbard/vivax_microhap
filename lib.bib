  author = {Nderu, David and Kimani, Francis and Karanja, Evaline and Thiong'o, Kelvin and Akinyi, Maureen and Too, Edwin and Chege, William and Nambati, Eva and Wangai, Laura N. and Meyer, Christian G. and Velavan, Thirumalaisamy P.},
  date = {2019-05},

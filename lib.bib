}

@article{deelderGeographicalClassificationMalaria2022,
  title = {Geographical Classification of Malaria Parasites through Applying Machine Learning to Whole Genome Sequence Data},
  author = {Deelder, Wouter and Manko, Emilia and Phelan, Jody E. and Campino, Susana and Palla, Luigi and Clark, Taane G.},

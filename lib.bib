}

@article{duforet-frebourgNonstationaryPatternsIsolationbyDistance2014,
  title = {Nonstationary {{Patterns}} of {{Isolation-by-Distance}}: {{Inferring Measures}} of {{Local Genetic Differentiation}} with {{Bayesian Kriging}}},

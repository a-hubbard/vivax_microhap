  pmcid = {PMC5777656},
  keywords = {Alleles,Animals,Anopheles,Computer Simulation,Female,Hepatocytes,Humans,Malaria Falciparum,Male,Meiosis,Models Genetic,Mosquito Vectors,notion,Oocysts,Pedigree,Plasmodium falciparum,Polymorphism Single Nucleotide,Probability,Recombination Genetic},

@unpublished{manriqueDesignValidationMultiplexed2023,
  type = {Poster},
  title = {Design and Validation of a Multiplexed Amplicon Sequencing Panel for {{Plasmodium}} Vivax Genomic Epidemiology in {{Latin America}}},
  author = {Manrique, Paulo and Schwabl, Philipp and Auburn, Sarah and Neafsey, Daniel E.},
  date = {2023-10},
  abstract = {Genomic surveillance of malaria parasites has demonstrated significant utility in recent years for tracking the spatial and temporal distribution of drug resistance markers, measuring population connectivity, detecting changes in transmission, and other use cases. The development of cost-effective genotyping panels for measuring genomic variation in Plasmodium vivax has lagged behind that of Plasmodium falciparum, despite the additional need in P. vivax to distinguish hypnozoite-driven relapse infections from reinfections or recrudescence following drug treatment. We have designed and validated an Illumina-based multiplexed PCR amplicon sequencing panel for P. vivax that incorporates over 150 amplicons covering a large collection of candidate drug resistance markers, amplicons used in other P. vivax genotyping panels, and amplicons capturing polymorphic regions with particular utility for spatial resolution of parasites from Latin America, a geographic focus of our work. The panel reliably generates genotypic data for patient samples with parasitemia levels as low as 10 parasites/ul, and is able to resolve genotypes for minor parasite strains in polyclonal infections, as determined with mock samples as well as authentic patient samples. We present in silico analyses of the enhanced capacity of this panel relative to two existing P. vivax panels to resolve the geographic population structure of parasites from Latin America. The panel does not make use of proprietary reagents and employs a protocol that is highly similar to that used for a comparable panel we previously developed for P. falciparum (AMPLseq). It also shares a common analysis workflow for amplicon data processing with the P. falciparum AMPLseq, and which may be downloaded from GitHub for local execution or accessed in a cloud-based analysis environment.},
  eventtitle = {{{ASTMH}}},
  venue = {Chicago},
  keywords = {notion},
  file = {/Users/alfred/Zotero/storage/RZV43Z4E/Screenshot from 2023-10-24 14-26-28.png}
}


  author = {Landguth, Erin L. and Fedy, Bradley C. and OYLER‐McCANCE, Sara J. and Garey, Andrew L. and Emel, Sarah L. and Mumma, Matthew and Wagner, Helene H. and Fortin, Marie-Josée and Cushman, Samuel A.},
  date = {2012},
  journaltitle = {Molecular Ecology Resources},

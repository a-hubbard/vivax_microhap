  pmcid = {PMC5932820},
  keywords = {Adult,Age Factors,Amplicon deep sequencing,Ethiopia,Genetic Variation,Geography,High-Throughput Nucleotide Sequencing,Humans,Kenya,Malaria Falciparum,Malaria Vivax,Male,Merozoite surface protein 1,Merozoite Surface Protein 1,Molecular epidemiology,Molecular Epidemiology,Multiplicity of infection,notion,Plasmodium,Plasmodium falciparum,Plasmodium vivax,Prevalence,Recurrence,Sex Factors,Time Factors,Within-host diversity},
  file = {/Users/alfred/Zotero/storage/JSBBEZQE/Zhong et al. - 2018 - Multiplicity and molecular epidemiology of Plasmod.pdf}
}


@article{abegazPrincipalsPrincipalComponents2019,
  title = {Principals about Principal Components in Statistical Genetics},
  author = {Abegaz, Fentaw and Chaichoompu, Kridsadakorn and Génin, Emmanuelle and Fardo, David W. and König, Inke R. and Mahachie John, Jestinah M. and Van Steen, Kristel},

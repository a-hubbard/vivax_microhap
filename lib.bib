@article{bannister-tyrrellHouseholdsHotspotsDefining2019,
  title = {Households or {{Hotspots}}? {{Defining Intervention Targets}} for {{Malaria Elimination}} in {{Ratanakiri Province}}, {{Eastern Cambodia}}},

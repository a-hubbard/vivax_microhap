}

@article{landguthQuantifyingLagTime2010,
  title = {Quantifying the Lag Time to Detect Barriers in Landscape Genetics},
  author = {Landguth, E. L. and Cushman, S. A. and Schwartz, M. K. and McKelvey, K. S. and Murphy, M. and Luikart, G.},

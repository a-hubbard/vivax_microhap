@article{colemanUsingSaTScanMethod2009,
  title = {Using the {{SaTScan}} Method to Detect Local Malaria Clusters for Guiding Malaria Control Programmes},
  author = {Coleman, Marlize and Coleman, Michael and Mabuza, Aaron M. and Kok, Gerdalize and Coetzee, Maureen and Durrheim, David N.},

@article{medleyHumanaidedNaturalDispersal2015,
  title = {Human-Aided and Natural Dispersal Drive Gene Flow across the Range of an Invasive Mosquito},
  author = {Medley, Kim A. and Jenkins, David G. and Hoffman, Eric A.},

}

@article{holzschuhMultiplexedDdPCRampliconSequencing2023,
  title = {Multiplexed {{ddPCR-amplicon}} Sequencing Reveals Isolated {{Plasmodium}} Falciparum Populations Amenable to Local Elimination in {{Zanzibar}}, {{Tanzania}}},
  author = {Holzschuh, Aurel and Lerch, Anita and Gerlovina, Inna and Fakih, Bakar S. and Al-Mafazy, Abdul-Wahid H. and Reaves, Erik J. and Ali, Abdullah and Abbas, Faiza and Ali, Mohamed Haji and Ali, Mohamed Ali and Hetzel, Manuel W. and Yukich, Joshua and Koepfli, Cristian},
  date = {2023-06-22},

  file = {/Users/alfred/Zotero/storage/BSYB6UZD/Meentemeyer et al. - 2011 - Landscape Epidemiology of Emerging Infectious Dise.pdf;/Users/alfred/Zotero/storage/BTIAEJNH/annurev-phyto-081211-172938.html}
}


  title = {Potential {{Opportunities}} and {{Challenges}} of {{Deploying Next Generation Sequencing}} and {{CRISPR-Cas Systems}} to {{Support Diagnostics}} and {{Surveillance Towards Malaria Control}} and {{Elimination}} in {{Africa}}},
  author = {Lyimo, Beatus M. and Popkin-Hall, Zachary R. and Giesbrecht, David J. and Mandara, Celine I. and Madebe, Rashid A. and Bakari, Catherine and Pereus, Dativa and Seth, Misago D. and Ngamba, Ramadhan M. and Mbwambo, Ruth B. and MacInnis, Bronwyn and Mbwambo, Daniel and Garimo, Issa and Chacky, Frank and Aaron, Sijenunu and Lusasi, Abdallah and Molteni, Fabrizio and Njau, Ritha and Cunningham, Jane A. and Lazaro, Samwel and Mohamed, Ally and Juliano, Jonathan J. and Bailey, Jeffrey A. and Ishengoma, Deus S.},

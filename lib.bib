}

@article{zhouInsecticideTreatedNetCampaign2016,
  title = {Insecticide-{{Treated Net Campaign}} and {{Malaria Transmission}} in {{Western Kenya}}: 2003-2015},
  shorttitle = {Insecticide-{{Treated Net Campaign}} and {{Malaria Transmission}} in {{Western Kenya}}},
  author = {Zhou, Guofa and Lee, Ming-Chieh and Githeko, Andrew K. and Atieli, Harrysone E. and Yan, Guiyun},
  date = {2016},

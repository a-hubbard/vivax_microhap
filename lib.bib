  pmcid = {PMC10944499},
  keywords = {Animals,Child,Female,Genomics,Humans,Malaria,Malaria Falciparum,Mozambique,Parasites,Plasmodium falciparum,Pregnancy,Prenatal Care},
  file = {/Users/alfred/Zotero/storage/8IXKMYXC/Brokhattingen et al. - 2024 - Genomic malaria surveillance of antenatal care users detects reduced transmission following eliminat.pdf}
}

@article{brownDistinguishingGeneFlow2021,
  title = {Distinguishing Gene Flow between Malaria Parasite Populations},
